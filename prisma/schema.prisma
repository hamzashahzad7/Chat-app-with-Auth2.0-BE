generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Message {
  id        Int      @id @default(autoincrement())
  content   String
  sender    String
  room      String
  sentiment String?  // ðŸ‘ˆ Added sentiment field (optional)
  createdAt DateTime @default(now())
}